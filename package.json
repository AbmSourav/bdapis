{
  "name": "bdapis",
  "version": "0.0.6",
  "description": "rest api service",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "dev": "nodemon app.js",
    "backend": "nodemon backend.js",
    "data:csv:to:json": "node dataProcessor/resource/parser.js",
    "db:create:tables": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma db push --force-reset",
    "db:migrate": "npm run db:migrate:divisions && npm run db:migrate:districts && npm run db:migrate:upazillas",
    "db:migrate:divisions": "node dataProcessor/migrations/divisions.js",
    "db:migrate:districts": "node dataProcessor/migrations/districts.js",
    "db:migrate:upazillas": "node dataProcessor/migrations/upazillas.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbmSourav/bdapis.git"
  },
  "keywords": [
    "restapi",
    "bd"
  ],
  "author": "Keramot UL Islam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AbmSourav/bdapis/issues"
  },
  "homepage": "https://github.com/AbmSourav/bdapis#readme",
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "@abmsourav/localdb": "^1.3.0",
    "csvtojson": "^2.0.10",
    "nodemon": "^2.0.20",
    "prisma": "^5.14.0"
  }
}
